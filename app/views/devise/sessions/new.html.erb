<main class="login">
  <div class="login-page">
    <div id="image-container">
    <%= image_tag "ajouclub.png", class: "logo" %>
    </div>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name) ) do |f| %>
    <div class="form">
      <form class="login-form">
        <div class="form-group">
          <%= f.email_field :email, autofocus: true, placeholder: "이메일", class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.password_field :password, autocomplete: "off", placeholder: "비밀번호", class: "form-control" %>
        </div>
        <div class="form-group">
          <%= f.submit "로그인", class: "btn btn-lg btn-block btn-success" %>
        </div>
        <div class="form-group">
          <a class="white-text btn btn-lg btn-block btn-warning">회원가입</a>
        </div>
      </form>
    </div>
      <% end %>
  </div>
</main>